#include <stdio.h>

int main(void)
{
	char var1 = 100;
	char* address1 = (char*) var1;
	printf("the address is %p", &var1);
	fflush(stdout);
	char data = address*;
	printf("\nthe stored data is: %c", data);
	address* = 65;
	printf("\nthe updated value is: %c", address*)


}
